{
    "name": "alkurn/yii2-ckeditor",
    "description": "CKEditor widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "alkurn",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "ckeditor"
    ],
    "homepage": "http://alkurn.info/ckeditor-widget",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alkurn Group",
            "email": "ganesh.alkurn@gmail.com",
            "homepage": "http://alkurn.info",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/alkurn/yii2-ckeditor/issues",
        "source": "https://github.com/alkurn/yii2-ckeditor"
    },
    "require": {
        "yiisoft/yii2": "^2.0",
        "ckeditor/ckeditor": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "alkurn\\ckeditor\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
